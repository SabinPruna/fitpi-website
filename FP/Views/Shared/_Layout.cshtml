<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>FitPi - Manage your life</title>
    
    <link href="../../Content/DarkTheme/assets/css/black-dashboard.css?v=1.0.0" rel="stylesheet" />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-navy fitpi-navbar">

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse left-navbar" id="navbarNav">
            <ul class="navbar-nav left-navbar-nav">
                <li class="nav-item active">
                    <a class="nav-link mr-20 ml-40 nav-border-bottom activities-nav-hover" href="#">ACTIVITIES <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link ml-50 mr-85 nav-border-bottom nutrition-nav-hover" href="#">NUTRITION</a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link nav-border-bottom budget-nav-hover" href="#">BUDGET</a>
                </li>
            </ul>
        </div>
        <a href="#" class="navbar-brand text-center">
            <img src="../../Content/Images/fitpi_logo_navbar.png" class="fitpi-logo" alt="">
        </a>

        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav ">
                <li class="nav-item active">
                    <a class="nav-link mr-85 nav-border-bottom timetable-nav-hover" href="#">TIMETABLE</a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link mr-85 nav-border-bottom worklog-nav-hover" href="#">WORKLOG</a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link mr-50 nav-border-bottom weather-nav-hover" href="#">WEATHER</a>
                </li>
            </ul>
        </div>
    </nav>

    <div class="container-fluid body-content">
        @RenderBody()
        <hr />
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/carouselScript")
    @RenderSection("scripts", required: false)

    <!--   Core JS Files   -->
    <script src="../../Content/DarkTheme/assets/js/core/jquery.min.js" type="text/javascript"></script>
    <script src="../../Content/DarkTheme/assets/js/core/popper.min.js" type="text/javascript"></script>
    <script src="../../Content/DarkTheme/assets/js/core/bootstrap.min.js" type="text/javascript"></script>
    <script src="../../Content/DarkTheme/assets/js/plugins/perfect-scrollbar.jquery.min.js"></script>
    <!--  Google Maps Plugin    -->
    @*<script src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE"></script>*@
    <!-- Chartist JS -->
    <script src="../../Content/DarkTheme/assets/js/plugins/chartjs.min.js"></script>
    <!--  Notifications Plugin    -->
    <script src="../../Content/DarkTheme/assets/js/plugins/bootstrap-notify.js"></script>
    <!-- Control Center for Material Dashboard: parallax effects, scripts for the example pages etc -->
    <script src="../../Content/DarkTheme/assets/js/black-dashboard.js?v=1.0.0" type="text/javascript"></script>


</body>
</html>
